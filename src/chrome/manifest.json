{
  "name" : "Trello Card Numbers",
  "description" : "Display card numbers in Trello.",
  "version" : "1.0",
  "manifest_version" : 2,
  "icons" : {
    "16" : "icon-16x16.png",
    "48" : "icon-48x48.png",
    "128" : "icon-128x128.png"
  },
  "content_scripts" : [
    {
      "matches" : [
        "http://trello.com/",
        "http://www.trello.com/",
        "https://trello.com/",
        "https://www.trello.com/"
      ],
      "css" : [
        "index.css"
      ]
    }
  ]
}
